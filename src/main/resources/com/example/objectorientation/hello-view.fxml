<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.objectorientation.controller.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Setup database" onAction="#onHelloButtonClick"/>
    <Button text="Show list of all notes" onAction="#onListButtonClick"/>
    <TextArea fx:id="notesTextArea" prefHeight="300.0" prefWidth="300.0"/>
    <Button text="Login" onAction="#loginUser"/>
    <Button text="Logout" onAction="#logoutUser"/>
</VBox>
